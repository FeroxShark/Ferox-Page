{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "globals": {
    "React": "readonly",
    "ReactDOM": "readonly",
    "DOMPurify": "readonly"
  },
  "rules": {
    "no-unused-vars": ["error", { "args": "none" }]
  }
}
